# plot
fig, ax = plt.subplots()
ax.plot(xs, ys, marker='o')
labels = ['A(-4,5)', 'B(0,7)', 'C(5,-5)', 'D(-4,-2)']
for (x, y), lab in zip([A, B, C, D], labels):
    ax.annotate(lab, (x, y))

ax.set_aspect('equal', adjustable='box')
ax.set_title('Quadrilateral ABCD')

# save and show
